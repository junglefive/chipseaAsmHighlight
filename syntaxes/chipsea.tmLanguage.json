			"patterns": [
				{
					"name": "constant.numeric",
					"match": "(?i)\\b\\d+\\b"
				},
				{
					"name": "constant.numeric",
					"match": "(?i)\\b0x[0-9A-Fa-f]+\\b"
				},
				{
					"name": "constant.numeric",
					"match": "(?i)\\b([01]+b)\\b"
				},
				{
					"name": "constant.numeric",
					"match": "(?i)\\b(0b[01]+)\\b"
				},
				{
					"name": "constant.numeric",
