		"comments": {
			"patterns": [
				{
					"comment": "EOL Comments",
					"name": "comment.line",
					"match": ";.*$"
				},
				{
					"comment": "Preprocessor",
					"name": "comment.line",
					"match": "//.*$"
				},
				{
					"comment": "Preprocessor",
					"name": "comment.block",
					"begin":"/\\*",
					"end":"\\*/"
				}
			]
		},
